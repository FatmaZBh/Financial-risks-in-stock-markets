      "cell_type": "code",
      "source": [
        "#  3. Define the time period\n",
        "\n",
        "start_date = datetime(2019, 1, 1)\n",
        "end_date = datetime.today()"
      ],
      "metadata": {
        "id": "5Pr0sec3UE6f"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# 4. Download daily price data for each ticker using yfinance\n",
        "\n",
        "'''raw_dataset = {}  # dictionary that will store one DataFrame per ticker\n",
        "\n",
        "for name,ticker in tickers.items():\n",
        "    # yf.download returns OHLCV data (Open, High, Low, Close, Adj Close, Volume)\n",
        "     df_ticker = yf.Ticker(\n",
        "        ticker)\n",
        "     df_historical=df_ticker.history(\n",
        "        start=start_date,\n",
        "        end=end_date,\n",
        "        interval=\"1d\" ,\n",
        "        # daily granularity: one row per trading day\n",
        "    )\n",
        "\n",
        "    # Reset the index so that 'Date' becomes a regular column instead of an index\n",
        "     df_historical = df_historical.reset_index()\n",
        "\n",
        "    # Store the DataFrame in the dictionary under the corresponding ticker symbol\n",
        "     raw_dataset[ticker] = df_historical\n",
        "     filename = f\"{name}_daily.csv\"\n",
        "     df_historical.to_csv(filename)\n",
        "     print(f\"Saved {filename}\")\n",
        "\n",
        "# Check one example to verify the structure\n",
        "aapl_df = pd.DataFrame(raw_dataset[\"AAPL\"])\n",
        "print(\"\\nPreview of Apple dataset:\")\n",
        "print(aapl_df.head())\n",
        "'''"
      ],
