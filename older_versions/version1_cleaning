    {
      "cell_type": "code",
      "source": [
        "import yfinance as yf\n",
        "import pandas as pd\n",
        "\n",
        "raw_dataset = {}  # will store one DataFrame per ticker\n",
        "\n",
        "for name, ticker in tickers.items():\n",
        "    print(f\"Downloading data for {name} ({ticker})...\")\n",
        "\n",
        "    # Option 1: use yf.download (simplest)\n",
        "    df_ticker = yf.download(\n",
        "        ticker,\n",
        "        start=start_date,\n",
        "        end=end_date,\n",
        "        interval=\"1d\",         # daily granularity\n",
        "        group_by=\"column\"      # avoid multi-index with ticker level\n",
        "    )\n",
        "\n",
        "    if isinstance(df_ticker.columns, pd.MultiIndex):\n",
        "      df_ticker.columns = df_ticker.columns.get_level_values(0)\n",
        "\n",
        "\n",
        "    # Put Date as a normal column instead of index\n",
        "    df_ticker = df_ticker.reset_index()\n",
        "\n",
        "    # Store in dictionary under the ticker symbol\n",
        "    raw_dataset[ticker] = df_ticker\n",
        "\n",
        "    # Save to CSV (no extra index column)\n",
        "    filename = f\"{name}_daily.csv\"\n",
        "    df_ticker.to_csv(filename, index=False)\n",
        "    print(f\"Saved {filename}\")\n",
        "\n",
        "# Check one example to verify the structure\n",
        "aapl_df = raw_dataset[\"AAPL\"]\n",
        "print(\"\\nPreview of AAPL dataset:\")\n",
        "print(aapl_df.head())\n"
      ],
      "metadata": {
